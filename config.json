{
    "EXPERIMENTS":
    {
        "PROJECT_NAME": "face_attribute"
    },

    "DATA":
    {
        "DATASET_NAME": "small_celeb_A",
        "INPUT_SHAPE": [224, 224, 3],
        "TRAIN_SIZE": 0.9,
        "BATCH_SIZE": 96,
        "NUM_WORKERS": 4
    },

    "CHECKPOINT":
    {
        "SAVE": true,
        "LOAD": true,
        "SAVE_ALL": true,
        "RESUME_NAME": "epoch_40.pt"
    },

    "EPOCH":
    {
        "START": 1,
        "EPOCHS": 1
    },

    "MODEL":
    {
        "NAME": "vgg16_bn",
        "PRETRAINED": false
    },

    "NN":
    {
        "DROPOUT": 0.5,
        "NUM_CLASSES": 2
    },

    "OPTIMIZER":
    {
        "NAME":  "Adam",
        "ARGS":
        {
            "lr": 1e-7,
            "amsgrad": true
        }
    },

    "EARLY_STOPPING":
    {
        "PATIENCE": 5,
        "MIN_DELTA": 0
    },

    "DEVICE":
    {
        "CUDA": true
    },

    "MISC":
    {
        "SEED": 12345,
         "APPLY_EARLY_STOPPING": true
    }
}