{
    "PROJECT_NAME": "face_attribute",

    "DATA":
    {
        "PATH": "/home/trong/Downloads/Local/Source/python/semester_6/face_attribute/small_celeb_A",
        "INPUT_SHAPE": [224, 224, 3],
        "TRAIN_SIZE": 0.9,
        "BATCH_SIZE": 132,
        "NUM_WORKERS": 4,
        "TRANSFORM":
        {
            "Resize":
            {
                    "size": [
                        224,
                        224
                    ],
                    "interpolation": "BICUBIC",
                    "antialias": true
                },

            "PILToTensor": {},

            "ToDtype":
            {
                "dtype": "int32",
                "scale": true
            },

            "RandomRotation":
            {
                "degrees": [-10, 10],
                "interpolation": "NEAREST"
            }
        },
        "TARGET_TRANSFORM": {}
    },

    "CHECKPOINT":
    {
        "SAVE_STRATEGY": "no",
        "SAVE_STEPS": 500,
        "SAVE_TOTAL_LIMIT": 5,
        "LOAD": false,
        "RESUME_NAME": "epoch_1.pt",
    },

    "METRICS":
    {
        "NAME_LIST": ["BinaryAccuracy", "BinaryF1Score"],
        "ARGS":
        {
            "0":
            {
               "threshold": 0.5
            },

            "1":
            {
               "threshold": 0.5
            }
        }
    },

    "MODEL":
    {
        "BASE": "vgg",
        "NAME": "vgg13",
        "PRETRAINED": false,
        "ARGS":
        {
            "dropout": 0.5,
            "num_classes": 1
        }
    },

    "OPTIMIZER":
    {
        "NAME": "Adam",
        "ARGS":
        {
            "lr": 1e-7,
            "amsgrad": true
        }
    },

    "LR_SCHEDULER":
    {
        "NAME": "CosineAnnealingWarmRestarts",
        "ARGS":
        {
            "T_0": 100,
            "T_mult": 3
        }
    },

    "LOSS":
    {
        "NAME": "BCELoss",
        "ARGS":
        {
            "reduction": "mean"
        }
    },

    "EARLY_STOPPING":
    {
        "APPLY": false,
        "PATIENCE": 5,
        "MIN_DELTA": 0
    },

    "SEED": 12345,
    "CUDA": true,
    "TRAINING_EPOCHS": 1
}