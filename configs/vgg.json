{
    "PROJECT_NAME": "face_attribute",

    "DATA":
    {
        "PATH": "/home/trong/Downloads/Local/Source/python/semester_6/face_attribute/small_celeb_A",
        "INPUT_SHAPE": [224, 224, 3],
        "TRAIN_SIZE": 0.9,
        "NUM_WORKERS": 4,
        "TRANSFORM":
        {
            "Resize":
            {
                "size": [224, 224],
                "interpolation": "BICUBIC",
                "antialias": true
            },

            "PILToTensor": {},

            "ToDtype":
            {
                "dtype": "int32",
                "scale": true
            },

            "RandomRotation":
            {
                "degrees": [-10, 10],
                "interpolation": "NEAREST"
            }
        },
        "TARGET_TRANSFORM": {}
    },

    "CHECKPOINT":
    {
        "SAVE_STRATEGY": "no",
        "SAVE_TOTAL_LIMIT": 5,
        "LOAD": false,
        "RESUME_NAME": "epoch_1.pt",
        "SAVE_ONLY_WEIGHT": true,
        "INCLUDE_CONFIG": false
    },

    "MODEL":
    {
        "BASE": "vgg",
        "NAME": "vgg13",
        "PRETRAINED": false,
        "ARGS":
        {
            "dropout": 0.5,
            "num_classes": 1
        }
    },

    "SOLVER":
    {
        "OPTIMIZER":
        {
            "Adam": {"lr": 1e-7, "amsgrad": true}
        },

        "LR_SCHEDULER":
        {
            "CosineAnnealingWarmRestarts": {"T_0": 100, "T_mult": 3}
        },

        "LOSS":
        {
            "BCELoss": {"reduction": "mean"}
        },

        "METRICS":
        {
            "BinaryAccuracy": {"threshold": 0.5},
            "BinaryF1Score": {"threshold": 0.5}
        }
    },


    "EARLY_STOPPING": {
        "APPLY": false,
        "ARGS": {
            "PATIENCE": 5,
            "MIN_DELTA": 0
        }
    },

    "LOGGING":
    {
        "STRATEGY": "epoch"
    },

    "SEED": 12345,
    "CUDA": true,
    "BATCH_SIZE": 132,
    "TRAINING_EPOCHS": 1,
    "EVALUATION_STRATEGY": "no",
}