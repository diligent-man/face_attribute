{
    //
    "EXPERIMENTS":
    {
        "PROJECT_NAME": "face_attribute"
    },

    "DATA":
    {
        "DATASET_NAME": "celeb_A",
        "INPUT_SHAPE": [3, 224, 224],
        "TRAIN_SIZE": 0.9,
        "BATCH_SIZE": 120,
        "NUM_WORKERS": 4,
        // Transform for X/ Variables
        "TRANSFORM":
        {
            "NAME_LIST": ["PILToTensor", "ToDtype", "RandomRotation"],
            "ARGS":
            {
                // Not need paras
                "0": {},

                "1":
                {
                    "dtype": "float32",
                    // normalize image from range [0, 255] into range [0, 1]
                    "scale": true
                },

                "2":
                {
                    "degrees": [-10, 10],
                    "interpolation": "NEAREST"
                }
            }
        }
    },

    "CHECKPOINT":
    {
        "SAVE": true,
        "LOAD": true,
        "SAVE_ALL": false,
        "RESUME_NAME": "epoch_41.pt"
    },

    "EPOCH":
    {
        "START": 1,
        "EPOCHS": 9
    },

    "METRICS":
    {
        "NAME_LIST": ["BinaryAccuracy", "BinaryF1Score"],
        "ARGS":
        {
            "0":
            {
               "threshold": 0.5
            },

            "1":
            {
               "threshold": 0.5
            }
        }
    },

    "SOLVER":
    {
        "MODEL":
        {
            "BASE": "vgg",
            "NAME": "vgg13",
            "PRETRAINED": false,
            "ARGS":
            {
                "dropout": 0.5,
                "num_classes": 1
            }
        },

        "OPTIMIZER":
        {
            "NAME": "Adam",
            "ARGS":
            {
                "lr": 1e-6,
                "amsgrad": true
            }
        },

        "LR_SCHEDULER":
        {
            "NAME": "CosineAnnealingWarmRestarts",
            "ARGS":
            {
                "T_0": 100,
                "T_mult": 3,
            }
        },

        "LOSS":
        {
            "NAME": "BCELoss",
            "ARGS":
            {
                "reduction": "mean"
            }
        },

        "EARLY_STOPPING":
        {
            "PATIENCE": 5,
            "MIN_DELTA": 0
        }
    },

    "MISC":
    {
        "SEED": 12345,
        "APPLY_EARLY_STOPPING": true,
        "CUDA": true
    }
}